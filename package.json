{
  "name": "@demissew/google-maps-vitest-mocks",
  "version": "2.7.6",
  "keywords": [
    "google",
    "maps",
    "vitest"
  ],
  "homepage": "https://github.com/demissew/google-maps-vitest-mocks",
  "bugs": {
    "url": "https://github.com/demissew/google-maps-vitest-mocks/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/demissew/google-maps-vitest-mocks.git"
  },
  "license": "Apache-2.0",
  "author": "Justin Poehnelt",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts",
